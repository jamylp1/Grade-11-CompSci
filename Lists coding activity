clothing_rack = []

clothing_rack.append("dress")
clothing_rack.append("trousers")
clothing_rack.append("socks")
clothing_rack.append("skirt")
clothing_rack.append("shirt")

print(clothing_rack)

print(clothing_rack[0:3])

clothing_rack.extend(["t-shirt", "shoes", "cap"])
print(clothing_rack)

clothing_rack.insert(4, "gloves")
print(clothing_rack)

clothing_rack.remove("shoes")
print(clothing_rack)

del clothing_rack[5]
print(clothing_rack)

last_item=clothing_rack.pop()
print(last_item)

print(clothing_rack)
if "dress" in clothing_rack:
    print("I have found my dress")
else:
    print("I cant find my dress")
    
print(clothing_rack.index("gloves"))

clothing_rack.sort()
print(clothing_rack)

clothing_rack.reverse()
print(clothing_rack)

for x in clothing_rack:
    print(x)
    


